{
  "Hubitat": {
    "WebSocketURL": "ws://echo.websocket.org",
    "InfluxDbURL": "http://192.168.99.100:8086",
    "InfluxDbDatabase": "test",
    "InfluxDbUsername": null,
    "InfluxDbPassword": null,
    "BatchInterval": 2
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "HubitatInfluxLogger.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}